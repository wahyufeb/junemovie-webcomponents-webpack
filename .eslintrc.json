{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle":"off",
        "no-tabs":"off",
        "no-console":"off",
        "import/no-extraneous-dependencies":"off"
    },
    "plugins": [
        "html"
    ]
}
